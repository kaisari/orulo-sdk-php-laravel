{
    "name": "jetimob/orulo-sdk-php-laravel",
    "description": "Laravel SDK for Orulo's API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Weingartner",
            "email": "hi@alanwgt.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "guzzlehttp/guzzle": "~6.0",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Jetimob\\Orulo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jetimob\\Orulo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jetimob\\Orulo\\OruloServiceProvider"
            ],
            "aliases": {
                "Orulo": "Jetimob\\Orulo\\Facades\\Orulo"
            }
        }
    }
}
